class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        vector<int> r;
        unordered_set<int> seen(nums.begin(), nums.end());
        int n = nums.size();
        for (int i = 1; i <= n; i++) {
            if (seen.find(i) == seen.end()) {
                r.push_back(i);
            }
        }
        return r;
    }
};
