class Solution {
public:
    int equalPairs(vector<vector<int>>& grid) {
        int count = 0;
        int n = grid.size();

        vector<vector<int>> row = grid;
        vector<vector<int>> col;

        for (int j = 0; j < n; j++) {
            vector<int> a;
            for (int i = 0; i < n; i++) {
                a.push_back(grid[i][j]);
            }
            col.push_back(a);
        }

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                if (row[i] == col[j]) {
                    count++;
                }
            }
        }

        return count;
    }
};
