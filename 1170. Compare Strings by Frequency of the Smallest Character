class Solution {
public:
    int getFreq(string s) {
        char mn = 'z';
        for (char c : s) mn = min(mn, c);

        int cnt = 0;
        for (char c : s) if (c == mn) cnt++;

        return cnt;
    }

    vector<int> numSmallerByFrequency(vector<string>& queries, vector<string>& words) {

        vector<int> fqQ(queries.size());
        vector<int> fqW(words.size());

        for (int i = 0; i < queries.size(); i++)
            fqQ[i] = getFreq(queries[i]);

        for (int i = 0; i < words.size(); i++)
            fqW[i] = getFreq(words[i]);

        vector<int> answer(queries.size(), 0);

        for (int i = 0; i < queries.size(); i++) {
            for (int j = 0; j < words.size(); j++) {
                if (fqQ[i] < fqW[j]) {
                    answer[i]++;
                }
            }
        }

        return answer;
    }
};
