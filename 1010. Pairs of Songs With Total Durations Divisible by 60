class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        int count = 0;
        int freq[60] = {0};


        for (int t : time) {
            int rem = t % 60;
            int complement = (60 - rem) % 60;
            count += freq[complement];
            freq[rem]++;
        }
        return count;
    }
};
