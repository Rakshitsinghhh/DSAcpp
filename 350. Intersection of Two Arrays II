class Solution {
public:
    vector<int> intersect(vector<int>& nums1, vector<int>& nums2) {
        unordered_map<int,int> seen;
        vector<int> ans;

        for(auto& ele : nums2){
            seen[ele]++;
        }

        for(auto& ele : nums1){
            if(seen[ele]>0){
                ans.push_back(ele);
                seen[ele]--;
            }
        }

        return ans;
        
    }
};
