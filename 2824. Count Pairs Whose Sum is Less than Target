class Solution {
public:
    int countPairs(vector<int>& nums, int target) {
        int count = 0;
        for(int i=0;i<nums.size();i++){
            for(int j=i;j<nums.size();j++){
                if( i>=0 && j>=0 && i<j){
                    if(nums[i]+nums[j]<target){
                        count++;
                    }
                }
            }
        }
        return count;
        
    }
};
