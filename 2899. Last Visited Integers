class Solution {
public:
    vector<int> lastVisitedIntegers(vector<int>& nums) {
        vector<int> seen;
        vector<int> answer;
        int k=0;

        for(int i=0;i<nums.size();i++){
            if(nums[i]==-1 ){
                k++;

                if(k<=seen.size()){
                    answer.push_back(seen[k-1]);
                }
                else{
                    answer.push_back(-1);
                } 
            }

            else{
                k=0;
                seen.insert(seen.begin(),nums[i]);
            }
        }

        return answer;
        
    }
};
