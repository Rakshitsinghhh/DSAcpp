class Solution {
public:
    int smallestNumber(int n, int t) {
        for(int i=n;i<=100;i++){
            int prod=1;
            int x=i;

            while(x!=0){
                prod *= x % 10;
                x/=10;


            }
            if(prod%t==0){
                return i;
            }
        }
        return 0;
    }
};
