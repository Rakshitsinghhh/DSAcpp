class Solution {
public:
    int findLucky(vector<int>& arr) {
        unordered_map<int,int> seen;
        int max=INT_MIN;

        for(auto ele :arr){
            seen[ele]++;
        }
        for(auto ele : seen){
            if(ele.second>max && ele.first==ele.second){
                max = ele.second;
            }   
        }


        if(max==INT_MIN) return -1;
        return max;
        
    }
};
