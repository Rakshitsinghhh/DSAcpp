class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {

        vector<int> answer;

        for(int i=0;i<nums1.size();i++){
            int ele = -1;
            bool state = false;

            for(int j=0;j<nums2.size();j++){

                if(nums2[j]==nums1[i]){
                    state=true;
                }

                else if(state && nums2[j]>nums1[i]){
                    ele=nums2[j];
                    break;
                }
            }
            answer.push_back(ele);
        }

        return answer;
        
    }
};
