class Solution {
public:
    bool hasSameDigits(string s) {
        while(s.size() != 2) {
            string a = "";
            for(int i = 0; i < s.size() - 1; i++) {
                int x = s[i] - '0';
                int y = s[i + 1] - '0';
                int ans = (x + y) % 10;
                a += (ans + '0');
            }
            s = a;
        }
        if(s.size() == 2 && s[0] == s[1]) {
            return true;
        }
        return false;
    }
};
